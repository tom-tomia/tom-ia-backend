{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "tomia": {
      "type": "text"
    },
    "slug": {
      "type": "uid"
    },
    "chats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "user"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "user"
    },
    "stripeCustomerId": {
      "type": "string"
    },
    "stripeSubscriptionId": {
      "type": "string"
    },
    "stripePriceId": {
      "type": "string"
    },
    "stripeCurrentPeriodEnd": {
      "type": "date"
    },
    "planId": {
      "type": "string"
    },
    "pendingDowngrade": {
      "type": "boolean",
      "default": false
    },
    "reoccuring": {
      "type": "boolean",
      "default": false
    },
    "cancelled": {
      "type": "boolean",
      "default": false
    },
    "plan_limit": {
      "type": "integer"
    },
    "school_info": {
      "type": "component",
      "repeatable": false,
      "component": "school.school"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    }
  }
}
